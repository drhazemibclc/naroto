{
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@naroto/config": "workspace:*",
    "@naroto/env": "workspace:*",
    "@types/express-session": "^1.18.2",
    "@types/node": "^25.3.1",
    "dotenv": "catalog:",
    "express-session": "^1.19.0",
    "husky": "latest",
    "lefthook": "latest",
    "lint-staged": "latest",
    "sherif": "latest",
    "turbo": "^2.8.11",
    "typescript": "catalog:",
    "ultracite": "7.2.4",
    "zod": "catalog:"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched",
      "ultracite fix"
    ],
    "package.json": [
      "sherif"
    ]
  },
  "name": "naroto",
  "packageManager": "pnpm@10.30.3",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "bump-deps": "pnpx -npm-check-updates --deep -u -x geist && pnpm install",
    "check": "biome check --write",
    "clean": "rm -rf .turbo node_modules && find . -name 'dist' -type d -prune -exec rm -rf {} +",
    "db:generate": "turbo -F @naroto/db db:generate",
    "db:push": "turbo -F @naroto/db db:push",
    "dev": "turbo dev",
    "fix": "ultracite fix",
    "lint:ws": "sherif -f",
    "prepare": "husky",
    "typecheck": "turbo typecheck",
    "ultra-check": "ultracite check",
    "ultra-fix": "ultracite fix"
  },
  "type": "module",
  "version": "0.0.0"
}