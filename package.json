{
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@naroto/config": "workspace:*",
    "@naroto/env": "workspace:*",
    "@types/express-session": "^1.18.2",
    "@types/node": "^25.3.2",
    "dotenv": "catalog:",
    "express-session": "^1.19.0",
    "lefthook": "^2.1.1",
    "lint-staged": "^16.2.7",
    "sherif": "^1.10.0",
    "turbo": "^2.8.11",
    "typescript": "catalog:",
    "ultracite": "7.2.4",
    "zod": "catalog:"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "package.json": "pnpm run lint:ws"
  },
  "name": "naroto",
  "packageManager": "pnpm@10.30.3",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "bump-deps": "pnpx npm-check-updates --deep -u -x geist && pnpm install",
    "check": "biome check --write",
    "clean": "rm -rf .turbo node_modules && find . -name 'dist' -type d -prune -exec rm -rf {} +",
    "db:generate": "turbo -F @naroto/db db:generate",
    "db:migrate": "turbo -F @naroto/db db:migrate",
    "db:pull": "turbo -F @naroto/db db:pull",
    "db:push": "turbo -F @naroto/db db:push",
    "db:reset": "turbo -F @naroto/db db:reset",
    "db:seed": "turbo -F @naroto/db db:seed",
    "dev": "turbo dev",
    "fix": "ultracite fix",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:ws": "sherif --fix ",
    "typecheck": "turbo typecheck",
    "ultra-check": "ultracite check",
    "ultra-fix": "ultracite fix"
  },
  "type": "module",
  "version": "0.0.0"
}