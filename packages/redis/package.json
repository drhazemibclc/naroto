{
  "dependencies": {
    "@naroto/logger": "workspace:*",
    "bull": "^4.16.5",
    "bullmq": "^5.70.1",
    "connect-redis": "^9.0.0",
    "ioredis": "^5.9.3",
    "rate-limit-redis": "^4.3.1",
    "tls": "^0.0.1",
